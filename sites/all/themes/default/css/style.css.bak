/* ============容器============== */
body{margin:0;padding:0;border:0;height:100%;text-align:left} 
#contain{position: relative;padding:0px;margin:0px auto;border:0; width:1002px; overflow: hidden}
#content {position:relative;margin:10px auto 10px auto;_margin:5px auto 5px auto;width:1002px;overflow: hidden} 
#bottom {position: relative;margin:0px auto;width:1002px;} 
#top {position:relative;margin:0px auto; overflow: hidden; width:1002px;} 
#bodyex {position: absolute;left:0;top:0px;margin:0} 



/* ==常用== */

div,li,td,p,select,input,textarea,a,button,input{font:12px Verdana, Arial, Helvetica, sans-serif;text-decoration: none}
ul{margin:0px;padding:0px;}
p { line-height:200%;}
a:link,a:visited,a:active{color:#505050;text-decoration: none;}
a.nomal:link,a.nomal:visited,a.nomal:active{color:#505050;text-decoration: underline;}
a:hover{color:#ff0000;text-decoration: underline;}

